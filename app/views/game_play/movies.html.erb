<h1><%= @movie.title %></h1><br>
<img src="<%=@movie.poster_url%>" height="400" width="300"><br>
<%= @movie.tagline %><br>
<%= @movie.overview %>
<ul>
<% @movie.quotes.each do |quote| %>
<audio controls>
  <source src="<%=quote.audio_path%>" type="audio/mpeg">
Your browser does not support the audio element.
</audio>
<li><%= quote.quote %></li>
<% end %>
</ul>